              <stringProp name="script">import groovy.json.JsonSlurper;

import groovy.json.JsonOutput;
import groovy.json.internal.LazyMap;

def jsonSlurper = new JsonSlurper();

definitionIds = jsonSlurper.parseText(vars.get(&quot;definitionIds&quot;));
permissionsIds = jsonSlurper.parseText(vars.get(&quot;permissionIds&quot;));

def commons = definitionIds.intersect(permissionsIds)
def difference = definitionIds.plus(permissionsIds)
difference.removeAll(commons)

AssertionResult.setFailureMessage(&quot;The list of definitions shows a definition to which the user has no access ${difference}.&quot;);
AssertionResult.setFailure(!permissionsIds.containsAll(definitionIds));


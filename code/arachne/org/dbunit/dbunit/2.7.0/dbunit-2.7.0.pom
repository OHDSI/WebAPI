    <!-- Database driver versions -->
    <derbyDriverVersion>10.4.1.3</derbyDriverVersion>
    <h2DriverVersion>1.1.118</h2DriverVersion>
    <hsqldbDriverVersion>1.8.0.1</hsqldbDriverVersion>
    <mysqlDriverVersion>5.1.6</mysqlDriverVersion>
